BUFLEN	case/getline/server_simple.cpp	15;"	d	file:
BUFLEN	server_simple.cpp	15;"	d	file:
createData	case/getline/client_simple.cpp	/^int createData(char * tmp, int off)$/;"	f
createData	client_simple.cpp	/^int createData(char * tmp, int off)$/;"	f
main	case/getline/client_simple.cpp	/^int main(int argc, char *argv[])$/;"	f
main	case/getline/server_simple.cpp	/^int main(int argc, char *argv[])$/;"	f
main	client_simple.cpp	/^int main(int argc, char *argv[])$/;"	f
main	server_simple.cpp	/^int main(int argc, char *argv[])$/;"	f
max	case/getline/client_simple.cpp	15;"	d	file:
max	case/getline/server_simple.cpp	14;"	d	file:
max	client_simple.cpp	15;"	d	file:
max	server_simple.cpp	14;"	d	file:
work	case/getline/client_simple.cpp	/^void * work(void * arg)$/;"	f
work	client_simple.cpp	/^void * work(void * arg)$/;"	f
